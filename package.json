{
  "name": "floorplans-workspace",
  "description": "Base workspace package",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "npm run clean --workspaces",
    "dev": "vite",
    "build": "npm run langium:generate && npm run build --workspaces && tsc && npx vite build",
    "langium:generate": "npm run --workspace language langium:generate",
    "langium:watch": "npm run --workspace language langium:watch",
    "test": "npm run --workspace language test"
  },
  "devDependencies": {
    "langium": "^4.0.0",
    "shx": "~0.4.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "volta": {
    "node": "20.19.2",
    "npm": "10.8.2"
  },
  "workspaces": [
    "language"
  ]
}
