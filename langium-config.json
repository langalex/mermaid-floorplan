{
  "projectName": "Floorplan",
  "languages": [
    {
      "id": "floorplan",
      "grammar": "floorplan.langium",
      "fileExtensions": [".fp"],
      "textMate": {
        "out": "syntaxes/floorplan.tmLanguage.json"
      },
      "railroad": {
        "out": "docs/syntax-diagram.html"
      }
    }
  ],
  "out": "src/language-server/generated",
  "chevrotainParserConfig": {
    "recoveryEnabled": true,
    "nodeLocationTracking": "full",
    "maxLookahead": 3
  }
}
